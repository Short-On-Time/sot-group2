{
  "name": "short-on-time",
  "version": "0.0.1",
  "description": "Herbs Team",
  "type": "module",
  "main": "client/app.js",
  "authors": [
    "Short On Time"
  ],
  "license": "MIT",
  "scripts": {
    "install": "cd client && npm i && cd ../server && npm i",
    "start-server": "cd server && npm start",
    "start-client": "cd client && npm start",
    "start": "npm-run-all --parallel start-server start-client",
    "build": "cd client && npm build",
    "test": "cd client && npm test",
    "eject": "cd client && npm eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Short-On-Time/sot-group2.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/Short-On-Time/sot-group2/issues"
  },
  "homepage": "https://github.com/Short-On-Time/sot-group2#readme",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
